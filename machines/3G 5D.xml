<machines>
	<machine>
		<name>3G 5D Cupcake +accel (Pololu XYZ, Mk5/6+Pololu Extruder)</name>
		<geometry type="cartesian">
			<!-- different pulleys on X and Y axii -->
			<axis id="x" length="100" maxfeedrate="9600" stepspermm="94.1397046"/>  <!-- sixteenth-step driver -->
			<axis id="y" length="100" maxfeedrate="9600" stepspermm="94.1397046"/>  <!-- sixteenth-step driver -->
			<axis id="z" length="100" maxfeedrate="600" stepspermm="2560"/> <!-- sixteenth-step driver -->
      <axis id="a" length="100000" maxfeedrate="5000" stepspermm="100.470957613814818"/>
		</geometry>
		<tools>
			<tool name="Mk5/6/6+" type="extruder" material="abs" floodcoolant="false" mistcoolant="false" fan="true" valve="false" collet="false" heater="true"  heatedplatform="true" motor_steps="400" stepper_axis="a"/>
		</tools>
		<clamps></clamps>
		<driver name="makerbot4g">
			<!-- optional, defaults to first serial port found.    <portname>COM1</portname> -->
			<!-- required: we need 8 bit and 38400 baud. -->
			<rate>38400</rate>
			<parity>8</parity>
			<!-- optional, defaults to 1.                          <databits>1</databits> -->
			<!-- optional, defaults to N.                          <stopbits>N</stopbits> -->
			<!--<debuglevel>5</debuglevel>-->
		</driver>
		<warmup>
		</warmup>
		<cooldown>
			(Turn off steppers after a build.)
			M18
		</cooldown>
	</machine>
